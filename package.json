{
  "name": "slackr",
  "version": null,
  "description": "Send incoming Slack messages like a boss!",
  "main": "index.js",
  "files": [
    "examples",
    "index.js",
    "test.js"
  ],
  "scripts": {
    "codacy-coverage": "codacy-coverage",
    "cover": "istanbul cover --report lcovonly ./node_modules/.bin/_mocha -- test.js",
    "greenkeeper-postpublish": "greenkeeper-postpublish",
    "lint": "eslint index.js test.js examples/",
    "nsp": "nsp check",
    "semantic-release": "semantic-release",
    "start": "supervisor index.js",
    "test": "mocha -b -c --check-leaks -R tap test.js",
    "test:watch": "mocha -w -b -c --check-leaks -R progress test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Starefossen/node-slackr.git"
  },
  "keywords": [
    "slack",
    "write",
    "post",
    "message",
    "messages",
    "incoming",
    "webhook",
    "webhooks"
  ],
  "author": "Hans Kristian Flaatten <hans@starefossen.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Starefossen/node-slackr/issues"
  },
  "homepage": "https://github.com/Starefossen/node-slackr#readme",
  "devDependencies": {
    "codacy-coverage": "^1.1.3",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^4.0.2",
    "eslint-plugin-import": "^1.11.1",
    "express": "^4.13.4",
    "greenkeeper-postpublish": "^1.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "nsp": "^2.4.0",
    "semantic-release": "^4.3.5",
    "supertest": "^1.2.0",
    "supervisor": "^0.11.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
